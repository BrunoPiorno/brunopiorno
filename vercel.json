{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "brunopiorno.com.ar" }
      ],
      "destination": "https://globalalora.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.brunopiorno.com.ar" }
      ],
      "destination": "https://globalalora.com/$1",
      "permanent": true
    }
  ]
}
