{
  "name": "estudio-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@google/generative-ai": "^0.24.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "framer-motion": "^10.16.4",
    "openai": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-slick": "^0.31.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^12.0.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node ./scripts/generate-static-pages.js && node ./scripts/generate-sitemap.mjs",
    "build:local": "react-scripts build && node ./scripts/generate-static-pages.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sitemap": "node ./scripts/generate-sitemap.mjs"
  },
  "reactSnap": {
    "inlineCss": true,
    "skipThirdPartyRequests": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/blog",
      "/en",
      "/en/blog"
    ],
    "crawl": false,
    "externalRoutes": "src/crawlableRoutes.js",
    "source": "build",
    "destination": "build",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "concurrency": 4,
    "puppeteerIgnoreHTTPSErrors": true,
    "removeBlobs": true,
    "fixWebpackChunksIssue": true,
    "removeStyleTags": false,
    "preloadImages": false,
    "cacheAjaxRequests": true,
    "http2PushManifest": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "eslint": "^8.53.0",
    "react-snap": "^1.23.0"
  }
}
